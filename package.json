    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "postcss": "8.4.47",
    "prettier": "^3.3.3",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.2",
    "wrangler": "^3.80.0"
