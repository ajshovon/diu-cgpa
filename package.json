{
  "name": "diu-cgpa-calculator",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "pages:deploy": "wrangler pages deploy .vercel/output/static  --project-name web",
    "api:deploy": "wrangler deploy --minify --name api app/api/[[...route]]/route.ts",
    "cf:build": "npm run lint && npx @cloudflare/next-on-pages",
    "cf:preview": "npm run cf:build && npx wrangler pages dev .vercel/output/static",
    "cf:deploy": "npm run cf:build && npm run api:deploy && npm run pages:deploy",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
    "format": "prettier --write \"app/**/*.ts\" \"app/**/*.tsx\" \"components/**/*.ts\" \"components/**/*.tsx\" \"config/**/*.ts\" \"types/**/*.ts\" \"utils/**/*.ts\" "
  },
  "dependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@hono/zod-validator": "^0.4.1",
    "@nextui-org/button": "2.0.38",
    "@nextui-org/card": "^2.0.34",
    "@nextui-org/chip": "^2.0.33",
    "@nextui-org/code": "2.0.33",
    "@nextui-org/input": "2.2.5",
    "@nextui-org/link": "2.0.35",
    "@nextui-org/navbar": "2.0.37",
    "@nextui-org/progress": "^2.0.34",
    "@nextui-org/switch": "^2.0.34",
    "@nextui-org/system": "2.2.6",
    "@nextui-org/table": "^2.0.40",
    "@nextui-org/theme": "2.2.11",
    "@react-aria/ssr": "3.9.6",
    "@react-aria/visually-hidden": "3.8.16",
    "clsx": "2.1.1",
    "framer-motion": "~11.11.1",
    "next": "14.2.14",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "^5.3.0",
    "sonner": "^1.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241004.0",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
